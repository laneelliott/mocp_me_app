{
  "name": "mocp.me",
  "version": "2.0.0",
  "description": "React based application to explore the works of the Museum of Contemporary Photography",
  "main": "index.js",
  "scripts": {
  	"install": "yarn install --modules-folder ./client/node_modules",
    "server": "nodemon server.js",
    "client": "node startup-scripts/start-client.js",
    "start": "concurrently \"node server.js\" \"npm run client\"",
    "build": "node startup-scripts/build.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "node-dev": "^3.1.3"
  },
  "repository": "https://github.com/laneelliott/mocp_me_app",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "path": "^0.12.7"
  }
}
